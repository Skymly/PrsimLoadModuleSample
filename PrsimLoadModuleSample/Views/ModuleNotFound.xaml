<UserControl x:Class="PrsimLoadModuleSample.Views.ModuleNotFound"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrsimLoadModuleSample.Views" 
             xmlns:prism="http://prismlibrary.com/" 
             xmlns:viewmodels="clr-namespace:PrsimLoadModuleSample.ViewModels" 
             prism:ViewModelLocator.AutoWireViewModel="True"
             d:DataContext="{d:DesignInstance Type=viewmodels:ModuleNotFoundViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock FontSize="36" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Run Text="{Binding Name,Mode=OneWay}" Foreground="MediumSlateBlue"/>
            <Run Text=" Module NotFound"/>
        </TextBlock>
    </Grid>
</UserControl>
